`define smlog(msg, cond, tag) \
    `log(self.class.name $ "::" $ GetFuncName() $ "(): " $ `msg, \
    `cond, \
    `if(`tag) name("SeedMutator-" $ string(`tag)) `else 'SeedMutator' `endif)

`define smlog(msg, cond, tag) \
    `log(default.class.name $ "::" $ GetFuncName() $ "(): " $ `msg, \
    `cond, \
    `if(`tag) name("SeedMutator-" $ string(`tag)) `else 'SeedMutator' `endif)
